<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import com.jfoenix.controls.JFXCheckBox?>
<?import com.jfoenix.controls.JFXToggleNode?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.FlowPane?>
<?import javafx.scene.layout.HBox?>
<?import org.kordamp.ikonli.javafx.FontIcon?>

<HBox maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" stylesheets="@../styles/expander-column-button.css" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <FlowPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="30.0" prefWidth="200.0" prefWrapLength="180.0" HBox.hgrow="NEVER">
         <children>
            <JFXCheckBox fx:id="checkBox" minHeight="-Infinity">
               <padding>
                  <Insets bottom="-1.0" left="-1.0" right="-1.0" top="-1.0" />
               </padding>
            </JFXCheckBox>
            <JFXButton>
               <graphic>
                  <ImageView fitHeight="24.0" fitWidth="24.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@../images/fox.png" />
                     </image>
                  </ImageView>
               </graphic>
            </JFXButton>
            <JFXToggleNode fx:id="toggleButton">
               <graphic>
                  <FontIcon iconLiteral="mdi-play" />
               </graphic>
            </JFXToggleNode>
            <Label fx:id="nameLabel" text="Label" />
         </children>
      </FlowPane>
   </children>
</HBox>
